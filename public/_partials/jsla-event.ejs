<section class="wrapper style2 container special-alt" style="background-color: <%= obj.color %>">
        <div class="row gtr-50">
          <div class="col-4 col-12-narrower">
            <header>
              <h1>
                  Come to
              </h1>
              <h2>
                <strong style="text-transform: none"><%= obj.name %></strong>
              </h2>
            </header>
            <% if(obj.drinksjs) {%>
            <p id="jsla-drinksjs-text">
              <strong>
                after the talks, around 9:15<br>at
                <a href="<%= public._data.drinkjs.url %>">
                  <span class="jsla-drinkjs-location">
                  <%= public._data.drinkjs.name %>
                </span>
              </a>
              </strong>
            </p>
            <% } else { %>
                <button>Reserve a seat now!</button>
                <% } %>
          </div>
          <div class="col-4 col-12-narrower imp-narrower">
  
                <% if(obj.map) {%> <iframe width="100%" height="250" id="gmap_canvas" src="https://maps.google.com/maps?q=<%=public._data.drinkjs.address%>&t=&z=13&ie=UTF8&iwloc=&output=embed"
                  frameborder="0" scrolling="no" marginheight="0" marginwidth="0" style="float: left;"></iframe>
                <% } else {%>
                    <p><%= obj.text %></p>
            <% } %>
          </div>
          <div class="col-4 col-12-narrower imp-narrower" style="text-align: center">
            <img src="<%= obj.image %>" height="250" style="width: auto; display: inline" />
            
            </div>
        </div>
      </section>